#include <stdio.h>

int main(){
	
	int a,b,c;
	
	printf("Digite tres numero inteiro:\n");
	scanf("%d\n%d\n%d", &a,&b,&c);
	
	if(a>b){
	
		int temp = b;
		b = a;
		a = temp;
}
	if(a>c){
	
		int temp=c;
		c=a;
		a=temp;		
}
	if(b>c){
	
		int temp=c;
		c=b;
		b=temp;
}
	printf("%d,%d,%d",a,b,c);				
	
return 0;
}
